{
	"name": "canadian-census",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"build": "npx tsc -p tsconfig.json",
		"statistics": "env-cmd -f .env.dev node --max-old-space-size=30000 dist/statistics.js",
		"create": "env-cmd -f .env.dev node --expose-gc --max-old-space-size=30000 dist/database.js",
		"dev": "env-cmd -f .env.dev node dist/server.js",
		"production": "env-cmd -f .env.prod node dist/server.js"
	},
	"license": "MIT",
	"dependencies": {
		"@types/express": "^4.17.21",
		"async": "^3.2.5",
		"better-sqlite3": "^11.0.0",
		"cli-progress": "^3.12.0",
		"csv-parse": "^5.5.6",
		"csv-parser": "^3.0.0",
		"express": "^4.19.2",
		"ioredis": "^5.4.1",
		"p-limit": "^5.0.0",
		"pbf": "^3.2.1",
		"pmtiles": "^3.0.5",
		"progress": "^2.0.3",
		"redis": "^4.6.14",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.7",
		"tippecanoe": "^0.6.0",
		"vtt": "^0.0.3"
	},
	"devDependencies": {
		"env-cmd": "^10.1.0",
		"typescript": "^5.4.5"
	}
